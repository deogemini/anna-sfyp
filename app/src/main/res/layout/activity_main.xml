<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EEABCF"
    tools:context=".MainActivity" >


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/allan_bold"
        android:text="Welcome to"
        android:textSize="60sp"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="254dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/allan_bold"
        android:text="MyHealth"
        android:textSize="60sp"
        tools:layout_editor_absoluteX="216dp"
        tools:layout_editor_absoluteY="312dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/allan_bold"
        android:text="Login"
        android:textSize="50sp"
        tools:layout_editor_absoluteX="156dp"
        tools:layout_editor_absoluteY="400dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:gravity="center_horizontal"
        android:inputType="textEmailAddress"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="526dp" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="627dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/allan_bold"
        android:text="Email"
        android:textSize="40sp"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="464dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/allan_bold"
        android:text="Password"
        android:textSize="40sp"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="582dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/allan_bold"
        android:text="Submit"
        android:textSize="40sp"
        app:backgroundTint="#23BE2E"
        tools:layout_editor_absoluteX="239dp"
        tools:layout_editor_absoluteY="701dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/allan_bold"
        android:text="CANCEL"
        android:textSize="40sp"
        app:backgroundTint="#F84141"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="701dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="200dp"
        android:layout_height="178dp"
        android:shape="oval"
        android:cropToPadding="true"
        app:srcCompat="@drawable/pregnant"
        tools:layout_editor_absoluteX="93dp"
        tools:layout_editor_absoluteY="43dp" />
</androidx.constraintlayout.widget.ConstraintLayout>